{
  "name": "@algolia/renderscript",
  "version": "1.5.4",
  "description": "A custom JavaScript rendering engine based on puppeteer",
  "main": "dist/index.js",
  "scripts": {
    "build": "yarn clean && ttsc",
    "ci:start": "ALLOW_LOCALHOST=true yarn start",
    "clean": "rm -rf dist/",
    "dev": "nodemon",
    "dev:run": "yarn build && NODE_ENV=development node dist/index.js",
    "docker:install": "node dist/lib/install.js",
    "docker:build": "./scripts/build.sh",
    "docker:release": "docker push algolia/renderscript",
    "lint": "eslint --ext=jsx,ts,tsx,js .",
    "release": "yarn docker:build && yarn docker:release",
    "start": "node --max-old-space-size=128 dist/index.js",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/algolia/renderscript.git"
  },
  "keywords": [
    "algolia",
    "puppeteer",
    "js renderer",
    "rendertron",
    "prerender"
  ],
  "author": "Algolia <contact@algolia.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/algolia/renderscript/issues"
  },
  "engines": {
    "node": "14.15.1",
    "yarn": "^1.19.0"
  },
  "homepage": "https://github.com/algolia/renderscript#readme",
  "devDependencies": {
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.0",
    "@types/express": "4.17.9",
    "@types/node-fetch": "2.5.7",
    "@types/node": "14.14.13",
    "@types/puppeteer-core": "4.0.0",
    "@types/uuid": "8.3.0",
    "@types/yauzl-promise": "2.1.0",
    "@typescript-eslint/eslint-plugin": "4.12.0",
    "@typescript-eslint/parser": "4.12.0",
    "concurrently": "5.3.0",
    "dotenv-safe": "8.2.0",
    "eslint-config-algolia": "16.0.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-config-standard": "16.0.2",
    "eslint-plugin-algolia": "2.0.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.1.0",
    "eslint": "7.18.0",
    "nodemon": "2.0.7",
    "prettier": "2.2.1",
    "semantic-release": "17.3.1",
    "tslint": "6.1.3",
    "ttypescript": "1.5.12",
    "typescript-transform-paths": "2.2.1",
    "typescript": "4.1.3"
  },
  "dependencies": {
    "@algolia/dns-filter": "1.1.11",
    "body-parser": "1.19.0",
    "express": "4.17.1",
    "node-fetch": "2.6.1",
    "puppeteer-core": "5.5.0",
    "stream-to-string": "1.2.0",
    "uuid": "8.3.2",
    "yauzl-promise": "2.1.3"
  }
}
